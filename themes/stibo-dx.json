{
  "name": "stibo-dx",
  "type": "dark",
  "colors": {
    "scrollbarSlider.background": "#0C1037",
    "scrollbarSlider.activeBackground": "#0C1037",
    "scrollbarSlider.hoverBackground": "#0C1037",
    "editor.background": "#141A5B",
    "editor.foreground": "#6E77DF",
    "editorLineNumber.foreground": "#6E77DF",
    "editorCursor.foreground": "#FFFFFF",
    "editor.findMatchBackground": "#CFEAF7",
    "editor.findMatchHighlightBackground": "#CFEAF7",
    "editor.lineHighlightBackground": "#181F6D",
    "editor.selectionBackground": "#181F6D",
    "editor.selectionHighlightBackground": "#181F6D",
    "input.background": "#0C1037",
    "input.border": "#0C1037",
    "focusBorder": "#0C1037",
    "activityBar.background": "#0C1037",
    "activityBar.foreground": "#FFFFFF",
    "sideBar.background": "#101549",
    "sideBarSectionHeader.background": "#0C1037",
    "statusBar.background": "#0C1037",
    "statusBar.foreground": "#CFEAF7",
    "statusBarItem.hoverBackground": "#101549",
    "titleBar.activeBackground": "#0C1037",
    "titleBar.activeForeground": "#181F6D",
    "titleBar.inactiveBackground": "#0C1037",
    "titleBar.inactiveForeground": "#ABB2BF",
    "editorGroupHeader.tabsBorder": "#111750",
    "editorGroupHeader.tabsBackground": "#111750",
    "tab.border": "#111750",
    "tab.activeBackground": "#141A5B",
    "tab.inactiveBackground": "#111750"
  },
  "tokenColors": [
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.section.property-list.begin.bracket.curly",
        "punctuation.section.property-list.end.bracket.curly",
        "punctuation.terminator.rule",
        "punctuation.accessor",
        "punctuation.section.embedded",
        "punctuation.separator",
        "punctuation.terminator.statement",
        "punctuation.definition.binding-pattern",
        "punctuation.support.type.property-name.begin",
        "punctuation.support.type.property-name.end"
      ],
      "settings": {
        "foreground": "#6E77DF"
      }
    },
    {
      "name": "Non-essentials",
      "scope": [
        "punctuation.definition.entity.css",
        "meta.tag.metadata.doctype.html",
        "cast.expr",
        "entity.name.type.ts",
        "keyword.operator.type",
        "meta.return.type",
        "meta.type.annotation",
        "meta.type.parameters",
        "support.type",
        "constant.language.import-export-all.ts",
        "keyword.control.as",
        "keyword.control.export",
        "meta.import",
        "keyword.control.at-rule.import",
        "punctuation.definition.block.sequence.item.yaml",
        "punctuation.definition.sequence.begin.yaml",
        "punctuation.definition.sequence.end.yaml",
        "punctuation.definition.mapping.begin.yaml",
        "punctuation.definition.mapping.end.yaml",
        "keyword.control.flow.block-scalar.folded.yaml",
        "keyword.control.flow.block-scalar.literal.yaml",
        "entity.other.document.begin.yaml"
      ],
      "settings": {
        "foreground": "#6E77DF"
      }
    },
    {
      "name": "Literals",
      "scope": [
        "constant.language",
        "constant.numeric",
        "string.quoted",
        "string.regexp",
        "string.template",
        "constant.other.timestamp.yaml",
        "string.unquoted.block.yaml",
        "string.unquoted.plain.in.yaml",
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#4DE8D1"
      }
    },
    {
      "name": "Attention seekers",
      "scope": [
        "comment",
        "keyword.other.debugger",
        "keyword.other.important",
        "variable.language.super",
        "variable.language.this",
        "variable.other.jsdoc",
        "storage.type.class.jsdoc",
        "meta.function.variable.css",
        "variable.css",
        "punctuation.definition.anchor.yaml",
        "entity.name.type.anchor.yaml",
        "punctuation.definition.alias.yaml",
        "variable.other.alias.yaml"
      ],
      "settings": {
        "foreground": "#FF6242"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.decrement",
        "keyword.operator.expression",
        "keyword.operator.increment",
        "keyword.operator.list",
        "keyword.operator.logical",
        "keyword.operator.pipe",
        "keyword.operator.relational",
        "keyword.operator.spread",
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#FD983D"
      }
    },
    {
      "name": "Function calls",
      "scope": [
        "meta.function-call",
        "new.expr",
        "punctuation.decorator",
        "support.function",
        "support.function.misc.css"
      ],
      "settings": {
        "foreground": "#CFEAF7"
      }
    },
    {
      "name": "Defaults",
      "scope": [
        "entity.name.type",
        "meta.definition.function",
        "meta.definition.method",
        "meta.objectliteral",
        "support.variable",
        "variable",
        "punctuation.section.function.begin.bracket.round.css",
        "punctuation.section.function.end.bracket.round.css",
        "support.type.vendored.property-name.css",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#CFEAF7"
      }
    }
  ]
}
