{
    "name": "vscode-theme-stibo-dx",
    "version": "0.0.3",
    "displayName": "Stibo DX",
    "description": "A Stibo DX theme for Visual Studio Code",
    "publisher": "christianhg",
    "scripts": {
        "build": "npm run prettier-production && node vscode-theme-stibo-dx.js",
        "prettier": "prettier --write '{*,**/*}.{md,js,ts}'",
        "prettier-production": "prettier --list-different '{*,**/*}.{md,js,ts}'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/christianhg/vscode-theme-stibo-dx"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "VSCode",
        "Themes"
    ],
    "engines": {
        "vscode": "^1.43.0"
    },
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Stibo DX",
                "uiTheme": "vs-dark",
                "path": "./themes/stibo-dx.json"
            }
        ]
    },
    "devDependencies": {
        "color": "^3.1.2",
        "jsonfile": "^6.0.1",
        "prettier": "^2.0.5"
    }
}
